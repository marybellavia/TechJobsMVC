@{ 
    // TODO #3 - Use the same list display from Views/Search/Index.cshtml 
}
<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

    @{
        /* using nested foreach loops to accesss dictionary from ViewBag.jobs (which is a list)
         * and then the inner loop is so I can access the kvp in each dictionary */
    }
    <h3>@ViewBag.jobs.Count Result(s)</h3>
    @foreach (Dictionary<string, string> job in ViewBag.jobs)
    {
        <table class="job-listing">
        @foreach (KeyValuePair<string, string> kvp in job)
        {

                <tr>
                    <td>@kvp.Key</td>
                    <td>@kvp.Value</td>
                </tr>

        }
        </table>
    }
